{
  "name": "create-vue-router",
  "version": "0.2.3",
  "description": "Add vue router to a vite project or any other existing vue 3 project not using vue cli.",
  "type": "module",
  "main": "dist/index.js",
  "bin": "dist/index.js",
  "scripts": {
    "watch": "npx rollup --config rollup.config.js --watch",
    "compile": "rm -rf dist && npx rollup --config rollup.config.js && cp -r ./src/config ./dist",
    "build": "rm -rf src/router && rm -rf src/views && node dist/index.js",
    "tsc": "tsc && cp -r ./src/config ./dist"
  },
  "keywords": [
    "vue",
    "vue router"
  ],
  "author": "Justin Logan",
  "license": "MIT",
  "dependencies": {
    "chalk": "^5.0.1",
    "vue-router": "^4.0.14"
  },
  "repository": {
    "type": "git",
    "url": ""
  },
  "devDependencies": {
    "@rollup/plugin-typescript": "^8.3.1",
    "@types/node": "^17.0.23",
    "eslint": "^8.13.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-plugin-import": "^2.26.0",
    "rollup": "^2.70.1",
    "rollup-plugin-terser": "^7.0.2",
    "ts-node": "^10.7.0",
    "typescript": "^4.6.3"
  }
}
