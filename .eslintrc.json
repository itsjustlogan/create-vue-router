{
  "env": {
    "browser": true,
    "commonjs": true,
    "es2021": true
  },
  "extends": ["airbnb-base"],
  "parserOptions": {
    "ecmaVersion": "latest"
  },
  "rules": {
    "semi": "off",
    "no-console": "off",
    "comma-dangle": "off",
    "prefer-destructuring": "off",
    "import/extensions": "off",
    "object-curly-newline": "off"
  }
}
